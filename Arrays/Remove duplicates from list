// Define a function that removes duplicates from an array of non negative numbers and returns it as a result.
let arr = [1,1,3,3,3]
function removeDuplicates(arr){
    let j=0;
    for(let i=0; i<arr.length;i++){
        if(arr[i]!==arr[i-1]){
            arr[j]=arr[i];
            j++
        }}
        arr.length = j;
    }
removeDuplicates(arr);
console.log(arr);